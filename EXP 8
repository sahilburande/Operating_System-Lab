#include <stdio.h>
int main()
{
int n, i;

printf("Enter no of processes");
scanf("%d", &n);

int at[20], bt[20], pr[20];
int ct[20], tat[20], wt[20];
int done[20] = {0};

for(i=0;i<n;i++)
{
printf("\nProcessesP%d\n",i+1);

printf("Arrival Time");
scanf("%d", &at[i]);

printf("Burst Time ");
scanf("%d", &bt[i]);

printf("Priority (Higher number = Higher Priority): ");
scanf("%d", &pr[i]);
}
int time = 0, completed = 0;

while(complete<n)
{
int max_priority = -1;
int index = -1;

for(i=0; i<n; i++)
{
if(at[i]<=time &&done[i]==0)
}
if(pr[i]>max_priority)
{
max_priority=pr[i];
index = i;
}
}
}
if(index !=-1)
{
time = time +bt[index];
ct[index]=time
tat[index]=ct[index]-at[index];
wt[index]=tat[index]-bt[index];

}

include <stdio.h>
int main()
{
int n, i;

printf("Enter no of processes");
scanf("%d", &n);

int at[20], bt[20], pr[20];
int ct[20], tat[20], wt[20];
int done[20] = {0};

for(i=0;i<n;i++)
{
printf("\nProcessesP%d\n",i+1);

printf("Arrival Time");
scanf("%d", &at[i]);

printf("Burst Time ");
scanf("%d", &bt[i]);

printf("Priority (Higher number = Higher Priority): ");
scanf("%d", &pr[i]);
}
int time = 0, completed = 0;

while(complete<n)
{
int max_priority = -1;
int index = -1;

for(i=0; i<n; i++)
{
if(at[i]<=time &&done[i]==0)
}
if(pr[i]>max_priority)
{
max_priority=pr[i];
index = i;
}
